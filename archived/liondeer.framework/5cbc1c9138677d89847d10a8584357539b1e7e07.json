{
    "manifests": {
        "liondeer/framework": {
            "manifest": {
                "bundles": {
                    "liondeer\\framework": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/",
                    "src/": "%SRC_DIR%/",
                    "test/": "d3test/"
                },
                "container": {
                    "app_env": "'%env(APP_ENV)%'",
                    "d3_app_name": "'%env(D3_APP_NAME)%'",
                    "d3_app_secret": "%env(D3_APP_SECRET)%",
                    "app_default_locale": "%env(APP_DEFAULT_LOCALE)%",
                    "d3_database_url": "%env(d3_db_url:D3_APP_NAME)%"
                },
                "post-install-output": [
                    "<fg=blue>Read</>Read the documentation at <comment>https://github.com/LionAndDeer/d3FrameworkBundle/blob/main/README.md</>"
                ]
            },
            "files": {
                "config/.gitkeep": {
                    "contents": [
                        ""
                    ],
                    "executable": false
                },
                "src/ControllerRegistrator.php": {
                    "contents": [
                        "<?php\r",
                        "\r",
                        "namespace App;\r",
                        "\r",
                        "use Liondeer\\Controller\\AbstractConfigFeatureController;\r",
                        "use Liondeer\\Controller\\AbstractDmsObjectExtensionController;\r",
                        "use Liondeer\\Controller\\AbstractFeatureController;\r",
                        "use Liondeer\\Exception\\LiondeerD3FrameworkException;\r",
                        "\r",
                        "class ControllerRegistrator\r",
                        "{\r",
                        "    /** @var AbstractFeatureController[] */\r",
                        "    private array $featureControllers = [];\r",
                        "    /** @var AbstractConfigFeatureController[] */\r",
                        "    private array $configFeatureControllers = [];\r",
                        "    /** @var AbstractDmsObjectExtensionController[] */\r",
                        "    private array $dmsObjectExtensionControllers = [];\r",
                        "\r",
                        "    //Im Konstruktor m\u00fcssen alle Feature- und ConfigFeature-Controller registriert werden\r",
                        "\r",
                        "    /**\r",
                        "     * @throws LiondeerD3FrameworkException\r",
                        "     */\r",
                        "    public function __construct(\r",
                        "    )\r",
                        "    {\r",
                        "        foreach (func_get_args() as $object) {\r",
                        "            if (is_a($object, AbstractFeatureController::class)) {\r",
                        "                array_push($this->featureControllers, $object);\r",
                        "            } elseif (is_a($object, AbstractConfigFeatureController::class)) {\r",
                        "                array_push($this->configFeatureControllers, $object);\r",
                        "            } elseif (is_a($object, AbstractDmsObjectExtensionController::class)) {\r",
                        "                array_push($this->dmsObjectExtensionControllers, $object);\r",
                        "            } else {\r",
                        "                $message = 'The class is neither \"'\r",
                        "                    . AbstractFeatureController::class\r",
                        "                    . '\" or \"'\r",
                        "                    . AbstractConfigFeatureController::class\r",
                        "                    .'\" or \"'\r",
                        "                    .AbstractDmsObjectExtensionController::class;\r",
                        "\r",
                        "                throw new LiondeerD3FrameworkException($message, 'LD-1000');\r",
                        "            }\r",
                        "        }\r",
                        "    }\r",
                        "\r",
                        "    public function getFeatureControllers(): array\r",
                        "    {\r",
                        "        return $this->featureControllers;\r",
                        "    }\r",
                        "\r",
                        "    public function getConfigFeatureControllers(): array\r",
                        "    {\r",
                        "        return $this->configFeatureControllers;\r",
                        "    }\r",
                        "\r",
                        "    public function getDmsObjectExtensionControllers(): array\r",
                        "    {\r",
                        "        return $this->dmsObjectExtensionControllers;\r",
                        "    }\r",
                        "}"
                    ],
                    "executable": false
                },
                "test/.gitkeep": {
                    "contents": [
                        ""
                    ],
                    "executable": false
                },
                "test/test.php": {
                    "contents": [
                        "<?php",
                        "",
                        "namespace public;",
                        "",
                        "class test",
                        "{",
                        "",
                        "}"
                    ],
                    "executable": false
                }
            },
            "ref": "5cbc1c9138677d89847d10a8584357539b1e7e07"
        }
    }
}
